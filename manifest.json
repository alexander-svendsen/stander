{
  "manifest_version": 2,

  "name": "Stander",
  "description": "An extension to give you notification when its time to stand a little and when it's ok to sit down again",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/check.png",
    "default_title": "Start timer"
  },
  "background": {
    "scripts": ["javascript/jquery-2.1.3.min.js","javascript/eventPage.js"],
    "persistent": false
  },
  "permissions": [
    "alarms",
    "storage"
  ]
}
